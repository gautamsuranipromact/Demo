default_platform(:android)

platform :android do
   desc "Assemble source"
    lane :assembleMaster do
      gradle(task: "assembleMasterRelease")
    end
    lane :assembleDev do
      gradle(task: "assembleDevRelease")
    end
    lane :increment_vc do
      increment_version_code(
        gradle_file_path: "./app/build.gradle",
      )
    end
end
